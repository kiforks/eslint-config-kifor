{
	"name": "eslint-config-kifor",
	"description": "eslint shareable config",
	"version": "1.1.0",
	"engines": {
		"node": ">=20.8.1"
	},
	"files": [
		"angular-template.js",
		"angular-typescript.js",
		"tests.js",
		"tests-jasmine.js",
		"tests-jest.js",
		"typescript.js"
	],
	"license": "MIT",
	"author": {
		"name": "Kifor Kostiantyn",
		"email": "kifor.ko@gmail.com"
	},
	"homepage": "https://github.com/kiforks/eslint-config-kifor#readme",
	"bugs": {
		"url": "https://github.com/kiforks/eslint-config-kifor/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kiforks/eslint-config-kifor.git"
	},
	"keywords": [
		"Kifor Kostiantyn",
		"code",
		"quality",
		"style",
		"lint",
		"linter",
		"eslint",
		"validate",
		"code style",
		"strict",
		"check",
		"checker",
		"enforce",
		"hint",
		"code lint",
		"kiforko",
		"kiforks",
		"kskifor",
		"eslint",
		"eslint-config",
		"eslint config",
		"kifor"
	],
	"dependencies": {
		"@angular-eslint/builder": "17.3.0",
		"@angular-eslint/eslint-plugin": "17.3.0",
		"@angular-eslint/eslint-plugin-template": "17.3.0",
		"@angular-eslint/schematics": "17.3.0",
		"@angular-eslint/template-parser": "17.3.0",
		"@stylistic/eslint-plugin-js": "^1.8.0",
		"@stylistic/eslint-plugin-ts": "^1.7.2",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"eslint-config-angular-strict": "^1.0.7",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jasmine": "^4.1.3",
		"eslint-plugin-jest": "^28.2.0",
		"eslint-plugin-max-params-no-constructor": "^0.0.4",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-scope": "8.0.1",
		"prettier": "^3.2.5"
	},
	"peerDependencies": {
		"eslint": "^8.57.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"husky": "^9.0.11",
		"kifor-prettier-config": "^1.0.5",
		"typescript": "5.4.5"
	},
	"scripts": {
		"update": "ncu -u && npm install",
		"eslint": "npx eslint **/*.scss --config .eslintrc.js --fix",
		"lint": "npx eslint '**/*.{ts,html,js}' --quiet --fix",
		"lint:check": "npx eslint '**/*.{ts,html,js}' --quiet",
		"format": "npx prettier '**/*.{ts,html,js,json,yml}' --write",
		"format:check": "npx prettier '**/*.{ts,html,js,json,yml}' --check",
		"prepare": "husky install",
		"prerelease": "npm run lint:check && npm run format:check"
	}
}
